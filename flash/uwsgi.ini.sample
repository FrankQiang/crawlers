[uwsgi]
workers         = 1 
max-requests    = 30000
enable-threads  = true
close-on-exec   = true
chdir           = /opt/dev/flash
module          = flash.wsgi:application
master          = true
processes       = 1
socket          = /tmp/flash.sock
chmod-socket    = 664
chown-socket    = www-data:www-data
vacuum          = true
